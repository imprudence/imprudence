		<combo_item name="MyHome" value="My Home">
			My Home
		</combo_item>
		<combo_item name="MyLastLocation" value="My Last Location">
			My Last Location
		</combo_item>
	</combo_box>     
	<check_box bottom="-44" control_name="ShowStartLocation" enabled="true" follows="left|top"
