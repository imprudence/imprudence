<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="worldmap" title="Mapa do mundo">
	<tab_container name="maptab">
		<panel label="Objetos" name="objects_mapview" />
		<panel label="Terreno" name="terrain_mapview" />
	</tab_container>
	<text name="land_for_sale_label">
		Terreno à venda
	</text>
	<text name="auction_label">
		Leilão
	</text>
	<text name="you_label">
		Você
	</text>
	<text name="home_label">
		Casa
	</text>
	<button label="Ir para casa" label_selected="Ir para casa" name="Go Home"
	     tool_tip="Teletransportar para sua casa" />
	<text name="classifieds_label">
		Classificados
	</text>
	<check_box label=" " name="class_chk" />
	<text name="person_label">
		Pessoa
	</text>
	<check_box label=" " name="people_chk" />
	<text name="infohub_label">
		Infohub
	</text>
	<check_box label=" " name="infohub_chk" />
	<text name="telehub_label">
		Telehub
	</text>
	<check_box label=" " name="telehubchk" />
	<text name="popular_label">
		Popular
	</text>
	<check_box label=" " name="popular_chk" />
	<text name="land_for_sale_label2">
		Terreno à venda
	</text>
	<check_box label=" " name="land_for_sale_chk" />
	<text name="events_label">
		Eventos
	</text>
	<check_box label=" " name="event_chk" />
	<text name="events_mature_label">
		Eventos (M)
	</text>
	<check_box label=" " name="event_mature_chk" />
	<combo_box label="Meus amigos" name="friend combo" tool_tip="Friend to Show on Map">
		<combo_item name="none_selected">
			Meus amigos
		</combo_item>
	</combo_box>
	<combo_box label="Meus pontos de referência" name="landmark combo"
	     tool_tip="Landmark to Show on Map">
		<combo_item name="none_selected">
			Meus pontos de referência
		</combo_item>
	</combo_box>
	<line_editor label="Localizar pelo nome da região" name="location"
	     tool_tip="Digite o nome de um região" />
	<button label="Pesquisar" name="DoSearch" tool_tip="Pesquisar por região" />
	<text name="search_label">
		Resultados de pesquisa:
	</text>
	<scroll_list name="search_results">
		<column label="" name="icon" />
		<column label="" name="sim_name" />
	</scroll_list>
	<text name="location_label">
		Local:
	</text>
	<spinner name="spin x" tool_tip="Coordenada X do local a mostrar no mapa" />
	<spinner name="spin y" tool_tip="Coordenada Y do local a mostrar no mapa" />
	<spinner name="spin z" tool_tip="Coordenada Z do local a mostrar no mapa" />
	<button label="Teletransportar" label_selected="Teletransportar" name="Teleport"
	     tool_tip="Teletransportar para local selecionado" />
	<button label="Mostrar destino" label_selected="Mostrar destino"
	     name="Show Destination" tool_tip="Centralizar mapa no local selecionado" />
	<button label="Limpar" label_selected="Limpar" name="Clear"
	     tool_tip="Parar rastreamento" />
	<button label="Mostrar minha posição" label_selected="Mostrar minha posição"
	     name="Show My Location" tool_tip="Centralizar mapa no local de seu avatar" />
	<slider label="Zoom" name="zoom slider" />
</floater>
