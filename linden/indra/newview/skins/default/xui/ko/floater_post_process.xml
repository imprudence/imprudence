<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="Post-Process Floater" title="사후 처리 설정">
	<tab_container name="Post-Process Tabs">
		<panel label="색상 필터" name="wmiColorFilterPanel">
			<check_box label="사용" name="wmiColorFilterToggle" />
			<text name="wmiColorFilterBrightnessText">
				밝기
			</text>
			<text name="wmiColorFilterSaturationText">
				채도
			</text>
			<text name="wmiColorFilterContrastText">
				명암
			</text>
			<text name="wmiColorFilterBaseText">
				명암 기본 색상
			</text>
			<slider label="R" name="wmiColorFilterBaseR" />
			<slider label="G" name="wmiColorFilterBaseG" />
			<slider label="B" name="wmiColorFilterBaseB" />
			<slider label="I" name="wmiColorFilterBaseI" />
		</panel>
		<panel label="나이트 비전" name="wmiNightVisionPanel">
			<check_box label="사용" name="wmiNightVisionToggle" />
			<text name="wmiNightVisionBrightMultText">
				빛 증폭
			</text>
			<text name="wmiNightVisionNoiseSizeText">
				노이즈 크기
			</text>
			<text name="wmiNightVisionNoiseStrengthText">
				노이즈 강도
			</text>
		</panel>
		<panel label="흐림" name="wmiBloomPanel">
			<check_box label="사용" name="wmiBloomToggle" />
			<text name="wmiBloomExtractText">
				광도 추출
			</text>
			<text name="wmiBloomSizeText">
				흐림 범위
			</text>
			<text name="wmiBloomStrengthText">
				흐림 강도
			</text>
		</panel>
		<panel label="특수" name="Extras">
			<button label="효과 로드" label_selected="효과 로드" name="PPLoadEffect" />
			<button label="효과 저장" label_selected="효과 저장" name="PPSaveEffect" />
			<line_editor label="효과 이름" name="PPEffectNameEditor" />
		</panel>
	</tab_container>
</floater>
