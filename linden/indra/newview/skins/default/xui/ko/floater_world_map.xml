	     tool_tip="Friend to Show on Map">
		<combo_item name="none_selected">
			온라인 친구 보기
		</combo_item>
	</combo_box>
	<combo_box label="랜드마크 보기" name="landmark combo"
	     tool_tip="Landmark to Show on Map">
		<combo_item name="none_selected">
			랜드마크 보기
		</combo_item>
	</combo_box>
	<line_editor label="지역명으로 검색" name="location" tool_tip="지역 이름 입력" />
	<button label="검색" name="DoSearch" tool_tip="지역 검색" />
	<text name="search_label">
		검색 결과:
	</text>
	<scroll_list name="search_results">
		<column label="" name="icon" />
		<column label="" name="sim_name" />
	</scroll_list>
	<text name="location_label">
		위치:
	</text>
	<spinner name="spin x" tool_tip="현 위치의 X 좌표를 지도에 표시" />
	<spinner name="spin y" tool_tip="현 위치의 Y 좌표를 지도에 표시" />
	<spinner name="spin z" tool_tip="현 위치의 Z 좌표를 지도에 표시" />
